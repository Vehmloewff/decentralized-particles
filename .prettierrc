{
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"endOfLine": "lf",
	"htmlWhitespaceSensitivity": "css",
	"insertPragma": false,
	"printWidth": 140,
	"proseWrap": "preserve",
	"requirePragma": false,
	"semi": true,
	"quoteProps": "as-needed",
	"jsxBracketSameLine": false,
	"singleQuote": true,
	"tabWidth": 4,
	"trailingComma": "es5",
	"useTabs": true
}
